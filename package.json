  "version": "0.2.5",
  "homepage": "http://github.com/greenify/biojs-vis-msa",
  "repository": {
    "type": "git",
    "url": "git://github.com/greenify/biojs-vis-msa"
  },
  "author": "greenify",
  "dependencies": {
    "backbone": "^1.1.2",
    "backbone-childs": "^1.0.2",
