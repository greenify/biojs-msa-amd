  "keywords": [
    "bio",
    "biojs"
  ],
  "main": "lib/index.js",
  "scripts": {
    "test": "node_modules/gulp/bin/gulp.js test",
    "prepublish": "npm test && node_modules/gulp/bin/gulp.js build && coffee --bare --compile --output lib/ src/",
    "watch": "mkdir -p build; node_modules/watchify/bin/cmd.js -v --debug -t coffeeify --extension='.coffee' browser.js -o build/msa.js; ( cd src/; find . -type f -not -iname '*.coffee' -exec cp '{}' '../lib/{}' ';' )",
    "build-browser": "node_modules/gulp/bin/gulp.js build-browser",
    "build-browser-min": "node_modules/gulp/bin/gulp.js build",
    "build-browser-cli": "mkdir -p build;  node_modules/browserify/bin/cmd.js -t coffeeify --extension='.coffee'  browser.js > build/msa.js",
    "sniper": "node_modules/biojs-sniper/bin/sniper ."
  },
  "license": "Apache 2",
  "shields": {
    "build": {
      "img": "https://travis-ci.org/greenify/biojs-vis-msa.svg?branch=master",
      "href": "https://travis-ci.org/greenify/biojs-vis-msa"
    },
    "tests": {
      "img": "https://codeclimate.com/github/greenify/biojs-vis-msa/badges/gpa.svg",
      "href": "https://codeclimate.com/github/greenify/biojs-vis-msa"
    }
  },
  "sniper": {
    "js": [
      "/build/msa.js"
    ],
    "css": [
      "/build/msa.css"
    ],
    "buildCSS": [
      "https://s3-eu-west-1.amazonaws.com/biojs/msa/latest/msa.css"
    ],
    "snippets": [
      "snippets"
    ],
    "first": "msa_show_menu"
  }
}
